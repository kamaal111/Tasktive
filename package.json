  "devDependencies": {
    "git-format-staged": "^3.0.0",
    "husky": "^8.0.1",
    "prettier": "^2.7.1"
  },
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
